<project toolsVersion="5.0">
  <!-- Only edit this file to pull kit dependencies. -->
  <!-- Put all extension-specific dependencies in `ext-deps.packman.xml`. -->
  <!-- This file contains shared Kit SDK dependencies used by most kit extensions. -->

  <!-- Import Kit SDK all-deps xml file to pull those deps of the same version as in Kit SDK -->
  <import path="../_build/${platform}/${config}/kit/dev/all-deps.packman.xml">
    <filter include="pybind11" linkPath="../_build/target-deps/pybind11" />
    <filter include="fmt" linkPath="../_build/target-deps/fmt" />
    <filter include="python" linkPath="../_build/target-deps/python" />
    <filter include="carb_sdk_plugins" linkPath="../_build/target-deps/carb_sdk_plugins" tags="non-redist" />
    <filter include="premake" linkPath="../_build/host-deps/premake" />
  </import>

</project>
